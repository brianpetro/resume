<div ng-controller="ResourceCtrl">
	<h1>Listing resources</h1>
	<table class="table table-bordered">
		<tr ng-repeat="site in resources">
			<td>{{site.site.url}}</td>
			<td>
				<ul>
					<li ng-repeat="tag in resources">
						{{tag.tags[0].name}}
					</li>
				</ul>
			</td>
		</tr>
	</table>
</div>
